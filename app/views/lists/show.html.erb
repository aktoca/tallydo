<h2><%= @list.name %></h2>

<div class="add"> 
  <% if @list.users.exists?(current_user) %>
  <%= percent_done(@list.tasks.mine(current_user)) %>%
  <% elsif current_user %>
  <%= button_to( "Add", addUser_path(@list.id)  ) %>
  <% end %>
</div> 


<%= render( partial: "tasks/doable", collection: @rankings) %>

<%= paginate @rankings %>
